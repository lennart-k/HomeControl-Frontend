    "@material/mwc-textfield": "^0.18.0",
    "homecontrol-js": "^1.2.0",
    "lodash": "^4.17.20",
    "marked": "^1.1.1",
    "material-design-icons": "^3.0.1",
    "typeface-inter": "^3.15.0",
    "typescript": "^3.9.7",
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.3",
    "vue-typed": "^2.1.4",
    "vuex": "^3.5.1",
    "vuex-pathify": "^1.4.1",
    "vuex-persist": "^2.2.0"
