{
  "name": "homecontrol-frontend",
  "version": "1.0.0",
  "description": "A frontend for HomeControl",
  "main": "src/main.ts",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "scripts": {
    "dev": "scripts/develop",
    "build": "scripts/build",
    "bump_version": "scripts/bump_version",
    "clean": "rm -rf .cache homecontrol_frontend/www dist build"
  },
  "pwaManifest": {
    "generateIconOptions": {
      "baseIcon": "./src/assets/branding/logo.svg",
      "sizes": [
        192,
        384,
        512,
        1024
      ],
      "genFavicons": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lennart-k/HomeControl-Frontend.git"
  },
  "keywords": [
    "home automation",
    "homecontrol",
    "frontend",
    "vuejs"
  ],
  "author": "Lennart K",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lennart-k/HomeControl-Frontend/issues"
  },
  "homepage": "https://github.com/lennart-k/HomeControl-Frontend#readme",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@types/marked": "^1.1.0",
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-loader": "^8.1.0",
    "babel-preset-latest": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-pwa-manifest": "^4.2.0"
  },
  "dependencies": {
    "@material/mwc-button": "^0.18.0",
    "@material/mwc-formfield": "^0.18.0",
    "@material/mwc-icon-button": "^0.18.0",
    "@material/mwc-linear-progress": "^0.18.0",
